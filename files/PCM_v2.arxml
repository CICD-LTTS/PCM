<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_4-3-0.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>PCM_Package</SHORT-NAME>
      <ELEMENTS>
        <APPLICATION-SW-COMPONENT-TYPE>
          <SHORT-NAME>PowertrainControlComponent</SHORT-NAME>
          <CATEGORY>ECU</CATEGORY>
          <ADMIN-DATA><DOC-REVISION>2.0.0</DOC-REVISION><CREATION-TIME>2025-11-02T08:15:00Z</CREATION-TIME><CHANGE-LOG>Added TorqueRequestIn; 10ms period; new runnable; updated parameter</CHANGE-LOG></ADMIN-DATA>
          <PORTS>
            <P-PORT-PROTOTYPE><SHORT-NAME>GearStatusOut</SHORT-NAME></P-PORT-PROTOTYPE>
            <R-PORT-PROTOTYPE><SHORT-NAME>DriverModeIn</SHORT-NAME></R-PORT-PROTOTYPE>
            <R-PORT-PROTOTYPE><SHORT-NAME>TorqueRequestIn</SHORT-NAME></R-PORT-PROTOTYPE>
          </PORTS>
          <INTERNAL-BEHAVIOR>
            <SHORT-NAME>PowertrainBehavior</SHORT-NAME>
            <EVENTS>
              <TIMING-EVENT><SHORT-NAME>Periodic10ms</SHORT-NAME><PERIOD>0.01</PERIOD></TIMING-EVENT>
            </EVENTS>
            <RUNNABLES>
              <RUNNABLE-ENTITY><SHORT-NAME>ShiftLogic</SHORT-NAME><CAN-BE-INVITED-CONCURRENTLY>false</CAN-BE-INVITED-CONCURRENTLY><MINIMUM-START-INTERVAL>0.008</MINIMUM-START-INTERVAL></RUNNABLE-ENTITY>
              <RUNNABLE-ENTITY><SHORT-NAME>ThermalManagement</SHORT-NAME><CAN-BE-INVITED-CONCURRENTLY>false</CAN-BE-INVITED-CONCURRENTLY><MINIMUM-START-INTERVAL>0.02</MINIMUM-START-INTERVAL></RUNNABLE-ENTITY>
            </RUNNABLES>
            <DATA-ELEMENTS>
              <VARIABLE-DATA-PROTOTYPE><SHORT-NAME>VehicleSpeed</SHORT-NAME><TYPE-TREF DEST="SW-DATA-DEF-PROPS">/BaseTypes/uint16</TYPE-TREF></VARIABLE-DATA-PROTOTYPE>
              <VARIABLE-DATA-PROTOTYPE><SHORT-NAME>RequestedTorque</SHORT-NAME><TYPE-TREF DEST="SW-DATA-DEF-PROPS">/BaseTypes/int16</TYPE-TREF></VARIABLE-DATA-PROTOTYPE>
            </DATA-ELEMENTS>
          </INTERNAL-BEHAVIOR>
          <PARAMETERS>
            <PARAMETER-DATA-PROTOTYPE><SHORT-NAME>MaxGear</SHORT-NAME><TYPE-TREF DEST="SW-DATA-DEF-PROPS">/BaseTypes/uint8</TYPE-TREF><SW-VALUATION><NUMERICAL-VALUE>8</NUMERICAL-VALUE></SW-VALUATION></PARAMETER-DATA-PROTOTYPE>
          </PARAMETERS>
        </APPLICATION-SW-COMPONENT-TYPE>
      </ELEMENTS>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
